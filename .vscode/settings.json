{
    "editor.inlineSuggest.enabled": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
        "editor.hover.enabled": true
    },
    "editor.rulers": [
        100
    ],
    "rust-analyzer.check.overrideCommand": [
        "cargo",
        "clippy",
        "--workspace",
        "--message-format=json",
        "--all-features",
        "--all-targets",
        "--",
        "-A",
        "incomplete-features"
    ],
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/crates/**/include",
        "${workspaceFolder}/target/include",
    ],
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "C_Cpp.intelliSenseEngine": "Tag Parser",
    "files.associations": {
        "cassert": "cpp",
        "array": "cpp",
        "string_view": "cpp",
        "initializer_list": "cpp",
        "utility": "cpp",
        "system_error": "cpp",
        "iostream": "cpp",
        "*.tcc": "cpp",
        "compare": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "random": "cpp",
        "cstdint": "cpp",
        "__config": "cpp",
        "atomic": "cpp",
        "memory_resource": "cpp",
        "stop_token": "cpp",
        "semaphore": "cpp",
        "ios": "cpp",
        "__locale": "cpp",
        "optional": "cpp",
        "ratio": "cpp",
        "functional": "cpp",
        "__bit_reference": "cpp",
        "__functional_base": "cpp",
        "__node_handle": "cpp",
        "algorithm": "cpp",
        "bitset": "cpp",
        "chrono": "cpp",
        "deque": "cpp",
        "__memory": "cpp",
        "iterator": "cpp",
        "limits": "cpp",
        "locale": "cpp",
        "memory": "cpp",
        "vector": "cpp",
        "iomanip": "cpp",
        "istream": "cpp",
        "mutex": "cpp",
        "ostream": "cpp",
        "numeric": "cpp",
        "sstream": "cpp",
        "streambuf": "cpp",
        "__functional_03": "cpp",
        "__hash_table": "cpp",
        "__tuple": "cpp",
        "thread": "cpp",
        "unordered_map": "cpp",
        "*.cu": "cuda-cpp",
        "*.cuh": "cuda-cpp"
    },
    "cmake.sourceDirectory": "/home/tamir/cuslop/crates/sys",
}